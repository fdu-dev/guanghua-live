<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.fudan.live.bean.Video" table="video">
		<id name="vid" type="java.lang.Integer">
			<column name="vid" />
			<generator class="increment" />
		</id>
		<property name="title" type="java.lang.String">
			<column name="title"  />
		</property>
		<property name="description" type="java.lang.String">
			<column name="description"  />
		</property>
		<property name="time" type="java.lang.Long">
			<column name="time"  />
		</property>
		<property name="formatTime" type="java.util.Date">
			<column name="format_time"  />
		</property>
		<property name="url" type="java.lang.String">
			<column name="url"  />
		</property>
		<property name="uid" type="java.lang.Integer">
			<column name="uid"  />
		</property>
		<property name="coverImg" type="java.lang.String">
			<column name="cover_img"  />
		</property>
		<property name="type" type="java.lang.Integer">
			<column name="type" />
		</property>
	</class>
</hibernate-mapping>
