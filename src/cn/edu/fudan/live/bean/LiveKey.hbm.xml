<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.fudan.live.bean.LiveKey" table="live_key">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="vid" type="java.lang.Integer">
			<column name="vid"  />
		</property>
		<property name="uid" type="java.lang.Integer">
			<column name="uid"  />
		</property>
		<property name="liveKey" type="java.lang.String">
			<column name="live_key"  />
		</property>
	</class>
</hibernate-mapping>
